{
  "name": "blink-module-assembler",
  "version": "2.0.0",
  "description": "BLINK - AI-powered full-stack project generator with admin dashboard",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.cjs",
    "db:init": "node -e \"require('./database/db.js').initializeDatabase().then(() => process.exit(0))\"",
